#include <stdio.h>

int main() {
    float myMark = 5.5;

    printf("My mark is %.1f\n", myMark);

    if(myMark > 9) {
        printf("I'm perfect\n");
    } else if(myMark > 7) {
        printf("I'm great\n");
    } else if(myMark > 5) {
        printf("I'm cool\n");
    } else if(myMark > 3) {
        printf("I think I'm cool\n");
    } else if(myMark > 1) {
        printf("Maybe I'm cool\n");
    } else {
        printf("Alright, alright\n");
    }
}
